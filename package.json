  "license": "MIT",
  "funding": "https://github.com/sponsors/iceywu",
  "homepage": "https://github.com/iceywu/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iceywu/utils.git"
  },
  "bugs": {
    "url": "https://github.com/iceywu/utils/issues"
  },
  "keywords": [
    "utils"
  ],
