    "p-limit": "^6.2.0",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^6.0.0",
    "throttle-debounce": "5.0.2",
